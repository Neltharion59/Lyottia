[t:cmd_region_message,n:0x04500c18]
{
	[common]
	{
		description = "";
	}
	[placement]
	{
	  p position = 0.0106697,0,-0.79808,0xb96b3c72;
	}
	[screen_report]
	{
		description = "'The Kahra, creatures as powerful as they were beautiful, used to call this place home. Now their bones decorate the floors, their flesh serves as my army. What chance would you think you stand?'";
	  b prepend_name = false;
	}
}
[t:cmd_region_message,n:0x04500c1a]
{
	[common]
	{
		description = "";
	}
	[placement]
	{
	  p position = 1.35529,0,0.0537643,0x1d030ef3;
	}
	[screen_report]
	{
		description = "'The bones of Kahra resist even my necromancy. But they won't defy me forever. My undead armies will be unstoppable then. Fear not, you will perish much sooner. You will be spared of seeing the world fall apart.'";
	  b prepend_name = false;
	}
}
[t:cmd_region_message,n:0x04500c1c]
{
	[common]
	{
		description = "";
	}
	[placement]
	{
	  p position = -0.627075,-2,3.83905,0x59c1a094;
	}
	[screen_report]
	{
		description = "'You seem a worthy opponent. You put a lot of good Kahra flesh to waste. I don't have much of it left to create new shails. Perhaps I might start with your own flesh.'";
	  b prepend_name = false;
	}
}
