	script	=[[
			sound play s_e_zapsplat_fantasy_fairy_fly_up_to_microphone_002_21527 at #SOURCE_POSITION dist 3 30;
			]];
}
[effect_script*]
{
	name	=lyottia_effect_32_magic_wall_cell;
	script	=[[
			randrange 0 6;
			set $temp #POP;
			if ( $temp == 1 )
			{
				set $orb_model chunks_ice_01;
			} else if ($temp == 2)
			{
				set $orb_model chunks_ice_02;
			} else if ($temp == 3)
			{
				set $orb_model chunks_ice_03;
			} else if ($temp == 4)
			{
				set $orb_model chunks_ice_04;
			} else
			{
				set $orb_model chunks_ice_05;
			}

			randrange 0 3;
			set $radius #POP;

			randrange 0 360;
			set $rotate_x #POP;
			randrange 0 360;
			set $rotate_y #POP;
			randrange 0 360;
			set $rotate_z #POP;

			sfx create orbiter #TARGET
				"must_update()model($orb_model)radius($radius)rotate($rotate_x,$rotate_y,$rotate_z)offset([0])";
            sfx snap_to_ground #PEEK;
			sfx start #POP;
	]];
}
[effect_script*]
{
	name	=lyottia_effect_32_magic_wall_column;
	script	=[[
			call lyottia_effect_32_magic_wall_cell s<[0],0,-5>;
			call lyottia_effect_32_magic_wall_cell s<[0],.5,-5>;
			call lyottia_effect_32_magic_wall_cell s<[0],1,-5>;
			call lyottia_effect_32_magic_wall_cell s<[0],1.5,-5>;
