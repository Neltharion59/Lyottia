{
	name	=lyottia_effect_32_statue_glb_wizard_17;
	script	=[[
			call lyottia_effect_32_statue_glb_wizard_base s<lyottia_ambient_32_statue_glb_wizard_17> u<0>;
			call lyottia_effect_32_statue_glb_wizard_base s<lyottia_ambient_32_statue_glb_wizard_17> u<90>;
