[effect_script*]
{
	name	=lyottia_effect_32_statue_glb_wizard_base;
	script	=[[
			sfx create orbiter #TARGET
				"must_update()color0(.1,.3,.1)model([0])radius(0)rotate(0,[1],0)";
			sfx start #POP;
	]];
}
