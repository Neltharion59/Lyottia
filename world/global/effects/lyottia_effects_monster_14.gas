[effect_script*]
{
	name	=lyottia_14_crystal_black;
	script	=[[
			sfx create orbiter #TARGET
				"must_update()color0(.1,.3,.1)model(lyottia_crystals_cav_01_black)radius(0)";
            sfx snap_to_ground #PEEK;
			sfx start #POP;
	]];
}
[effect_script*]
{
	name	=lyottia_zepheryl_fire_1;

	script	=[[
			set $color 1,.941,.4;

			sfx create orbiter #TARGET 
				"must_update()scale(.4)tin(0)tout(1)color0($color)radius(0)
				bone_orient()offset(0,-.5,-.5)[1]";
			sfx attach_point #PEEK @body_mid target;
			set $orb #PEEK;
			sfx start #POP;

			sfx create orbiter #TARGET 
				"must_update()texture(b_sfx_024)scale(.4)tin(0)
				tout(1)color0($color)radius(0)bone_orient()[1]";
			sfx rat #PEEK;
			sfx target #PEEK $orb;
			sfx start #POP;
			
			sfx create fire #TARGET_KB 
				"must_update()scale(1)ts(0.3)velocity(0,10,0)accel(0,-10,0)
				line()color0($color)color1(0,0,0)count(25)min_displace(0)
				max_displace(0)flamesize(.5)[1]";
			sfx rat #PEEK;
			sfx target #PEEK $orb;
			sfx start #POP;

			pause .2;
			
			sfx create explosion #TARGET
				"srate(0.01)omni_dir()ts(.05)scale(.15)vmin(8)vmax(8)scale_range(.375,.425,0)
				texture(b_sfx_024)color0($color)color1(0,0,0)
				radius(.2)fade_range(12,12,0)count(12)";
			sfx rat #PEEK;
			sfx target #PEEK $orb;
			sfx start #POP;
			
			sfx create sparkles #TARGET 
				"color0(1,.7,.7)psize(.6)radius(.3)ts(1.5)yvel(-.1)count(12)
				texture(b_sfx_024)offset(0,.2,0)";
			sfx rat #PEEK;
			sfx target #PEEK $orb;
			sfx start #POP;
			
			sfx create sray #TARGET 
				"count(24)color0($color)
				color1(0,0,0)lmin(.1)lmax(.5)";
			sfx attach_point #PEEK @body_mid target;
			sfx rat #PEEK;
			sfx target #PEEK $orb;
			sfx start #POP;


			sfx create cylinder #TARGET
			"must_update()alpha(.3)texture(b_sfx_armor_fireshot)hp0(0,0,0)
			hp1(1,1,0)spin(19)scale(.25)
			color0(1,1,1)offset(0,-.5,0)";
			sfx rat #PEEK;
			sfx target #PEEK $orb;
			sfx start #POP;
			
			sfx create cylinder #TARGET
			"must_update()alpha(.3)texture(b_sfx_armor_fireshot)hp0(.25,.25,0)
			hp1(.75,.75,0)spin(42)
			scale(1)color0($color)offset(0,-.5,0)";
			sfx rat #PEEK;
			sfx target #PEEK $orb;
			sfx start #POP;

			pause .1;

			sfx create cylinder #TARGET
			"must_update()alpha(.3)texture(b_sfx_armor_fireshot)hp0(1,1,0)
			hp1(0,0,0)spin(-43)
			scale(.375)color0(1,1,1)offset(0,-.5,0)";
			sfx rat #PEEK;
			sfx target #PEEK $orb;
			sfx start #POP;


		]];
}
[effect_script*]
{
	name	=lyottia_zepheryl_die_1;

	script	=[[
			set $color 1,.941,.4;

			sound play s_e_smash_rockbeast at #SOURCE_POSITION dist 1 10;
			
			sfx create explosion #TARGET
			"scale(.5)texture(b_sfx_024)ivel(0,1,0)omni_dir()
			color0($color)color1(0,0,0)dur(4)count(15)ts(.8)collide()";
			sfx attach_point #PEEK @body_mid target;
			sfx start #POP;
			
			sfx create orbiter #TARGET 
				"dur(.2)tin(0)tout(.2)scale(.25)color0(1,1,1)iphi(0)
				radius(0)";
			sfx attach_point #PEEK @body_mid target;
			sfx start #POP;

			sfx create fire #TARGET 
				"scale(.5)dur(.2)color0($color)color1(0,0,0)
				count(10)alphafade(1)flamesize(2)max_radius(3)instant()
				velocity(0,1,0)accel(0,0,0)ts(.7)";
			sfx attach_point #PEEK @body_mid target;
			sfx start #POP;


            sfx create explosion #TARGET 
	            "must_update()texture(b_sfx_024)omni_dir()vmin(5)
	            vmax(6)ts(.5)count(50)dur(3)scale(1)color0($color)
	            color1(0,0,0)rvel(0,1,0)ivel(0,0,0)";
			sfx start #POP;			
			sfx start #POP;			
			]];
}
