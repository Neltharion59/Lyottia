				"must_update()offset(0,2,0)count(120)radius($radius2)
				color0(1,.4,.1)color1(0,0,0)alphafade($fade)flamesize($size)
				max_radius(2)velocity(0,50,0)accel(0,-50,0)min_displace(-2)
				max_displace(6)[0]";
			set $fire2_pillar1 #POP;
