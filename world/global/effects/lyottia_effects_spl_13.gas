			sfx start $bolt;

			sfx create lightning #TARGET_KB 
				"must_update()scale(4.75)bolt_life(1)subd(100.0)minsubd(50.0)mindisplace(-6.5)maxdisplace(6.5)
				color0(.8,1,.8)[1]";
