			// Pillar 4
			
			//frandrange .2 .3
			sfx create orbiter #TARGET 
				"must_update()invisible()iphi(0)radius($radius1)iphi(.9)
				free()smooth()";
			set $base_pillar4 #POP;
			sfx snap_to_ground $base_pillar4;
