			set $height2 4;
			set $height3 6;

			set $radius1 5;
			set $radius2 1.2;
			set $speed 20;

			set $fade .65;

			set $size 5;

			sound play s_e_burning_large at #TARGET_POSITION dist 30 40 loop;
